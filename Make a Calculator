
#include <iostream>
using namespace std;


main()
{
	char op;
	float a1,a2;

	
	cin >> op;

	cin >> a1 >> a2;

	
	switch (op) {
		
		
		case '+':
			cout << a1 + a2;
			break;
		

		case '-':
			cout << a1 - a2;
			break;
		
		case '*':
			cout << a1 * a2;
			break;
		
		case '/':
			cout << a1 / a2;
			break;
		
		
		default:
			cout << "Error! operator is not correct";
			break;
	} 
	return 0;
}
